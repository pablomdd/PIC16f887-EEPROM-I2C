}



void i2c_waitForIdle()
{
while (( SSPCON2 & 0x1F ) | R_W ) {};
}



void i2c_start()
{
i2c_waitForIdle();
SEN=1;
}



void i2c_repStart()
{
i2c_waitForIdle();
RSEN=1;
}



void i2c_stop()
{
i2c_waitForIdle();
PEN=1;
}



int i2c_read( unsigned char ack )
{
unsigned char i2cReadData;

i2c_waitForIdle();

RCEN=1;

i2c_waitForIdle();

i2cReadData = SSPBUF;

i2c_waitForIdle();

if ( ack )
{
ACKDT=0;
}
else
{
ACKDT=1;
}
ACKEN=1;

return( i2cReadData );
}



unsigned char i2c_write( unsigned char i2cWriteData )
{
i2c_waitForIdle();
SSPBUF = i2cWriteData;

return ( ! ACKSTAT );
}

